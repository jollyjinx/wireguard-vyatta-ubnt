type: txt
help: Script or command executed before the interface goes down
val_help: txt; Command
val_help: txt; Executable script in /config/scripts


begin:  echo $(date|sed -e 's/\n//;') "wireguard/node.tag/down-command/node.def: begin: action: $COMMIT_ACTION arg: $VAR(@)" >> /tmp/wg.log

create: echo $(date|sed -e 's/\n//;') "wireguard/node.tag/down-command/node.def: create:action: $COMMIT_ACTION arg: $VAR(@)" >> /tmp/wg.log

delete: echo $(date|sed -e 's/\n//;') "wireguard/node.tag/down-command/node.def: delete:action: $COMMIT_ACTION arg: $VAR(@)" >> /tmp/wg.log

end:    echo $(date|sed -e 's/\n//;') "wireguard/node.tag/down-command/node.def: end:   action: $COMMIT_ACTION arg: $VAR(@)" >> /tmp/wg.log
